{
  "name": "myoperator-ui",
  "version": "0.0.3",
  "description": "CLI for adding myOperator UI components to your project",
  "type": "module",
  "exports": "./dist/index.js",
  "bin": {
    "myoperator-ui": "./dist/index.js"
