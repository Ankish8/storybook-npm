{
  "name": "myoperator-ui",
  "version": "0.0.45",
  "description": "CLI for adding myOperator UI components to your project",
  "type": "module",
  "exports": "./dist/index.js",
  "bin": {
    "myoperator-ui": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate-registry": "node scripts/generate-registry.js",
    "validate-registry": "node scripts/validate-registry.js",
    "verify-build": "node scripts/verify-build.js",
    "build": "npm run generate-registry && tsup src/index.ts --format esm --dts && npm run verify-build",
    "dev": "tsup src/index.ts --format esm --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build && npm run validate-registry"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "ora": "^8.0.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.0",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "myoperator",
    "ui",
    "components",
    "cli",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ankish8/storybook-npm.git"
  }
}
